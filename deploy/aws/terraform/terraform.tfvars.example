# =============================================================================
# RDF-StarBase Terraform Variables
# =============================================================================
# Copy this file to terraform.tfvars and customize as needed
# =============================================================================

aws_region      = "us-east-1"
environment     = "demo"
app_name        = "rdf-starbase"
container_image = "rxcthefirst/rdf-starbase:latest"
container_port  = 8000
task_cpu        = 512     # 0.5 vCPU
task_memory     = 1024    # 1 GB
desired_count   = 1
